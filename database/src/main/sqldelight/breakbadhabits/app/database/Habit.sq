CREATE TABLE IF NOT EXISTS Habit (
  id          INTEGER NOT NULL PRIMARY KEY,
  name        TEXT    NOT NULL,
  iconId      INTEGER NOT NULL
);

insert:
INSERT INTO Habit
VALUES (?, ?, ?);

deleteById:
DELETE FROM Habit WHERE id = ?;

selectAll:
SELECT * FROM Habit;

selectById:
SELECT * FROM Habit WHERE id = ?;

countWithName:
SELECT count(*) FROM Habit WHERE name = ?;